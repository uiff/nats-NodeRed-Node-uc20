include "variable.fbs";

namespace weidmueller.ucontrol.hub;

/// A provider that provides variables to consumers.
table Provider {
  /// The id of the provider.
  id: string (required);
}

/// A list of providers that provide variables to consumers.
table ProviderList {
  /// The list of providers.
  items: [Provider];
}
